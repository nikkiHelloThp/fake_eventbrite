<% @events.each do |event| %>
		<%= event.title %> <%= event.start_date %>
		<%= link_to 'Show', super_admin_event_path(event), class: "btn btn-primary" %>
		<%= link_to 'Edit', edit_super_admin_event_path(event), class: "btn btn-warning" %>
		<%= link_to 'Delete', super_admin_event_path(event), method: :delete, class: "btn btn-danger", data: {confirm: 'Are you sure?'} %>
		<hr>
<% end %>

<%= link_to 'Create', new_super_admin_event_path, class: "btn btn-success" %>